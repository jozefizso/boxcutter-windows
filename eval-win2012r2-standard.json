        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ],
        [
          "setextradata",
          "{{.Name}}",
          "VBoxInternal/CPUM/CMPXCHG16B",
          "1"
        ]
      ],
